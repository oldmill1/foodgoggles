// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model LogEntry {
  id            String   @id @default(uuid())
  fats          Float
  sugars        Float
  carbohydrates Float
  proteins      Float
  calories      Float
  notes         String
  slug          String
  timestamp     DateTime @default(now())
  
  @@map("log_entries")
}
